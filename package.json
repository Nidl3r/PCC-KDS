    "concurrently": "^9.2.0",
    "firebase-tools": "^14.12.0",
    "live-server": "^1.2.2",
    "rimraf": "^6.0.1",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@firebase/rules-unit-testing": "^5.0.0",
    "firebase": "^12.1.0",
    "firebase-admin": "^13.4.0"
  }
}
